class Solution {
    public int maxProduct(int[] nums) {
        if(nums.length==1)return nums[0];
        int max=nums[0];
        int i=0;
       for(i=0;i<nums.length;i++){
        int pro=1;
        for(int j=i;j<nums.length;j++){
            pro=pro*nums[j];
            if(max<pro){
                max=pro;
            }
        }

       }
        return max;
    }
}
